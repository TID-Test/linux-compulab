/dts-v1/;

#include "ucm-imx8m-plus.dts"

/ {
    model = "CompuLab UCM-iMX8M-Plus";
    compatible = "compulab,ucm-imx8m-plus", "fsl,imx8mp";

    memory@40000000 {
        device_type = "memory";
        reg = <0x0 0x40000000 0 0x16000000>,
              <0x1 0x58000000 0 0x68000000>;
    };

    chosen {
        bootargs = "console=ttymxc1,115200 earlycon root=/dev/mmcblk1p2 rootwait rw";
    };
};
